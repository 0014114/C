#include <stdio.h>
 
int main()
{
    int base, exponent;
 
    long long result = 1;
 
    printf("base: ");
    scanf("%d", &base);
 
    printf("exponent: ");
    scanf("%d", &exponent);
 
    while (exponent != 0)
    {
        result *= base;
        --exponent;
    }
    printf("结果：%lld", result);
 
    return 0;
}
